@model PatrocinioZoneProyectoV1.Models.Oferta

@{
    ViewData["Title"] = "Eliminar";
}

<h1 class="display-5">Eliminar</h1>

<h3>Está seguro de eliminar el registro?</h3>
<div>
    <h4>Oferta</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Patrocinador)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Patrocinador.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Costo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Costo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Club)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Club.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ZonaDePatrocinio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ZonaDePatrocinio.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="OfertaId" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
