@model IEnumerable<PatrocinioZoneProyectoV1.Models.ZonaPatrocinio>

@{
    ViewData["Title"] = "Index";
}

<h1>Zona de Patrocinio</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tamanio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoReservado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ubicacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tamanio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoReservado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ubicacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
